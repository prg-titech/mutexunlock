package main

import (
	"fmt"
	"sync"
)

func _(a bool) {
	var mu1 sync.Mutex

	for a {
		mu1.Lock()
		fmt.Println("hello")
		mu1.Unlock() // want "missing unlock"
	}
}

func _(l []int) int {
	var mu1 sync.Mutex

	for k, v := range l {
		mu1.Lock()
		if v == 0 {
			mu1.Unlock()
			return k // want "missing unlock"
		}
		mu1.Unlock() // want "missing unlock"
	}
	return -1 // OK
}

func _(l []int) int {
	var mu1 sync.Mutex

	for _, v := range l {
		mu1.Lock()
		if v == 0 {
			fmt.Println("To Avoid Nodes is empty")
			mu1.Unlock() // want "missing unlock"
			break
		}
		mu1.Unlock() // want "missing unlock"
	}

	return -1 // OK
}
